{
  "name": "roomieai-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --format esm,cjs --sourcemap --dts --clean",
    "start": "node dist/index.js",
    "migrate": "prisma migrate dev",
    "seed": "prisma db seed",
    "test": "vitest run",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "typecheck": "tsc --noEmit"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "5.16.1",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "firebase-admin": "12.6.0",
    "zod": "3.23.8",
    "jsonwebtoken": "9.0.2",
    "morgan": "1.10.0",
    "express-rate-limit": "7.4.0"
  },
  "devDependencies": {
    "@faker-js/faker": "9.0.0",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "9.0.6",
    "@types/node": "20.14.9",
    "@types/cors": "2.8.17",
    "@types/morgan": "1.9.9",
    "prisma": "5.16.1",
    "tsup": "8.1.0",
    "tsx": "4.16.2",
    "typescript": "5.5.3",
    "vitest": "1.6.0",
    "supertest": "7.0.0",
    "eslint": "9.7.0"
  }
}

